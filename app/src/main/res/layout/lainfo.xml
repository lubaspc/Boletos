<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
                                             android:layout_height="match_parent" android:orientation="vertical">



    <TextView
            
            android:text="TextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/idd"/>
    <ImageButton
            android:layout_width="match_parent"
            android:layout_height="wrap_content" app:srcCompat="@drawable/ic_launcher_background"
            android:id="@+id/IMG2"/>
    <EditText
            android:text="TextView"
            android:textSize="20sp"
            android:textColor="#000000"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/Noombre"/>
    <EditText
            android:text="TextView"
            android:textSize="20sp"
            android:textColor="#000000"
            android:inputType="number"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/Edadd"/>
    <TextView
            android:text="TextView"
            android:textSize="20sp"
            android:textColor="@color/colorAccent"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/FechaIDA"/>
    <Spinner
            android:layout_width="match_parent"
            android:layout_marginHorizontal="50dp"
            android:layout_height="wrap_content" android:id="@+id/spinner"/>
    <TextView
            android:text="TextView"
            android:textSize="20sp"
            android:textColor="@color/colorAccent"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/FechaRetorno"/>
    <Spinner
            android:layout_width="match_parent"
            android:layout_marginHorizontal="50dp"
            android:layout_height="wrap_content" android:id="@+id/spinner2"/>
    <TextView
            android:text="TextView"
            android:textSize="20sp"
            android:textColor="#000000"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/FechaCompra"/>

    <TextView
            android:text="TextView"
            android:textSize="20sp"
            android:textColor="#000000"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/CiudadS"/>
    <TextView
            android:text="TextView"
            android:textSize="20sp"
            android:textColor="@color/colorAccent"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/CiudadR"/>
    <Spinner
            android:layout_width="match_parent"
            android:layout_marginHorizontal="50dp"
            android:layout_height="wrap_content" android:id="@+id/spinner3"/>

    <TextView
            android:text="TextView"
            android:layout_width="match_parent"
            android:textSize="20sp"
            android:textColor="#000000"
            android:layout_marginTop="10dp"
            android:layout_height="wrap_content" android:id="@+id/Costo"/>
</LinearLayout>